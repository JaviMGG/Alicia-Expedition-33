  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7a7268", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#c8a84e", "fontStyle": "bold" }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#e6c47a" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#c0c0c0", "fontStyle": "italic" }
    },
    {
      "scope": "variable",
      "settings": { "foreground": "#e8e0d0" }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#a02020" }
    },
    {
      "scope": "entity.name.type",
      "settings": { "foreground": "#8b1a1a" }
    },
    {
      "scope": ["markup.bold", "markup.heading"],
      "settings": { "foreground": "#c8a84e", "fontStyle": "bold" }
    },
    {
      "scope": "markup.italic",
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": "markup.inserted",
      "settings": { "foreground": "#8b9e6b" }
    },
    {
      "scope": "markup.deleted",
      "settings": { "foreground": "#a02020" }
    }
  ],
  "editor.semanticHighlighting.enabled": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Alicia: Keywords (public, static, int)",
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier",
          "keyword.control"
        ],
        "settings": {
          "foreground": "#c8a84e",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Alicia: Nombre de la función (suma)",
        "scope": [
          "entity.name.function",
          "variable.function",
          "support.function"
        ],
        "settings": {
          "foreground": "#c0c0c0",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Alicia: Variables y parámetros",
        "scope": ["variable", "variable.other", "variable.parameter"],
        "settings": {
          "foreground": "#e8e0d0"
        }
      },
      {
        "name": "Alicia: Números y constantes",
        "scope": ["constant.numeric", "constant.language"],
        "settings": {
          "foreground": "#a02020"
        }
      },
      {
        "name": "Alicia: Operadores y puntuación",
        "scope": [
          "keyword.operator",
          "punctuation",
          "meta.brace",
          "punctuation.definition.parameters"
        ],
        "settings": {
          "foreground": "#d4d4d4"
        }
      },
      {
        "name": "Forzar color comentarios",
        "scope": "comment",
        "settings": {
          "foreground": "#7a7268",
          "fontStyle": "italic"
        }
      }
    ]
  }
}
